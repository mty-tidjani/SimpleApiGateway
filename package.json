  "description": "",
  "main": "app.js",
  "scripts": {
    "tsc": "tsc",
    "start": "set debug=* && ts-node-dev --respawn --transpileOnly ./core/app.ts",
    "dev": "ts-node-dev --respawn --transpileOnly ./core/app.ts",
    "prod": "tsc && node ./build/app.js",
