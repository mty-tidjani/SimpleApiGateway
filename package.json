    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fast-gateway": "^2.3.12",
    "http-proxy-middleware": "^1.0.3",
    "jsonwebtoken": "^8.5.1",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
